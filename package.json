{
  "name": "ember-stopwatch",
  "version": "0.2.1",
  "description": "Stopwatches, timers, clocks, oh my!",
  "keywords": ["ember-addon", "stopwatch", "timer", "clock"],
  "repository": "https://github.com/tzellman/ember-stopwatch.git",
  "license": "MIT",
  "author": "tzellman",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "clean": "rimraf dist coverage package*.ember-try .eslintcache",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "purge": "npm run clean && rimraf node_modules",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.23.1",
    "ember-cli-htmlbars": "^5.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^1.0.2",
    "@ember/test-helpers": "^2.1.4",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "@sinonjs/fake-timers": "^6.0.1",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.10.1",
    "ember-cli": "~3.24.0",
    "ember-cli-code-coverage": "^1.0.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-moment-shim": "^3.8.0",
    "ember-cli-postcss": "^6.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-composable-helpers": "^4.4.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-moment": "^8.0.1",
    "ember-page-title": "^6.0.3",
    "ember-qunit": "^5.1.1",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.24.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.17.0",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-ember": "^10.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "loader.js": "^4.7.0",
    "micromatch": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "qunit": "^2.13.0",
    "qunit-dom": "^1.6.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.9.6",
    "tracked-built-ins": "^1.0.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://tzellman.github.io/ember-stopwatch/"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged --config .prettierrc"
    }
  }
}
